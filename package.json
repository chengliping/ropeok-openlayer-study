{
  "name": "ropeok-openlayer-study",
  "version": "3.0.4",
  "private": true,
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint --fix --ext .js,.vue --ignore-path .gitignore .",
      "git add"
    ]
  },
  "scripts": {
    "start": "vue-cli-service serve",
    "api": "set NODE_ENV=development&& node ./bin/www",
    "build": "vue-cli-service build",
    "build&zip": "vue-cli-service build && npm run zip",
    "zip": "node bin/zip.js",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "publish-to-server": "npm run build && npm run to-server",
    "to-server": "node ./bin/publish-script.js",
    "download-nginx-config": "node ./bin/download-nginx-config.js"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "element-ui": "^2.13.1",
    "html5-storage": "^1.0.3",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "ol": "^6.13.0",
    "rk-cookie": "^1.0.1",
    "rk-encrypt": "^1.0.4",
    "rk-util": "^0.0.7",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "archiver": "^4.0.1",
    "axios": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "express": "~4.17.1",
    "http-errors": "~1.7.3",
    "http-proxy-middleware": "^0.20.0",
    "jest": "^26.0.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.2.4",
    "node-ssh": "^8.0.0",
    "supertest": "^4.0.2",
    "vue-template-compiler": "^2.6.11",
    "worker-loader": "^2.0.0",
    "ws": "^7.3.0"
  }
}
